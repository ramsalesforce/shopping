<apex:page standardController="Opportunity" extensions="GetSelectedOppController" recordSetVar="opportunities" contentType="application/vnd.ms-excel#TempExcel.xls" cache="true">
    <apex:pageBlock >
        <apex:pageBlockTable value="{!oppList}" var="opp">
            <apex:column value="{!opp.Name}"/>
            <apex:column value="{!opp.StageName}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>